/*
    At Most One
*/

asm
    new T
    new F
    put T, true
    put F, false
end

n = 4;

for i = 0 to n do
    asm
        new X[i] // global variables
    end
end

def at_most_one(m)
    for i = 0 to m - 1 do
        for j = i + 1 to m do
            asm
                new U // local variables
                new V
                not X[i], U
                not X[j], V
                or U, V, T
            end
        end
    end
end


at_most_one(n);